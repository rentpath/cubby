  "scripts": {
    "dev": "next dev",
    "build": "yarn generate:nexus && yarn generate:graphql && next build",
    "start": "next start",
    "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\"",
    "postinstall": "yarn prisma-init && yarn generate",
    "prisma-init:clean": "rimraf prisma/migrations && rimraf prisma/dev.db",
    "prisma-init:migrate": "prisma migrate dev --name init",
    "prisma-init:seed": "prisma db seed --preview-feature",
    "prisma-init": "yarn prisma-init:clean && yarn prisma-init:migrate && yarn prisma-init:seed",
    "generate:prisma": "prisma generate",
    "generate:nexus": "NEXUS_SHOULD_EXIT_AFTER_GENERATE_ARTIFACTS=true yarn ts-node --transpile-only src/lib/schema",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "generate": "yarn generate:prisma && yarn generate:nexus && yarn generate:graphql"
  },
  "dependencies": {
    "@cubbyjs/react": "file:../../packages/react",
    "@prisma/client": "2.27.x",
    "add": "^2.0.6",
    "apollo-server": "^3.1.1",
    "apollo-server-micro": "^3.1.1",
    "cookie": "^0.4.1",
    "graphql": "^15.5.1",
    "graphql-request": "^3.5.0",
    "graphql.macro": "^1.4.2",
    "micro": "^9.3.4",
    "next": "^11.0.1",
    "nexus": "^1.1.0",
    "nexus-prisma": "^0.30.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sqlite3": "^5.0.2",
    "typescript": "^4.3.5",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@babel/core": "^7",
    "@graphql-codegen/cli": "1.21.8",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-graphql-request": "^3.2.5",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@types/cookie": "^0.4.1",
    "@types/node": "^16.4.6",
    "autoprefixer": "^10.3.1",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "^7.31.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "prisma": "2.27.x",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.7",
    "ts-node": "^10.1.0"
  }
